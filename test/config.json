{
	/**
	 * Доступные браузеры для выполнения тестов.
	 * Порядок использования браузеров соответствует порядку конфигурации.
	 *
	 * Параметр перебивается переменной окружения `SELENIUM_BROWSER`.
	 *
	 * Значение по умолчанию firefox, возможные значения:
	 *
	 * - chrome
	 * - firefox
	 * - edge
	 * - ie
	 */
	"browsers": [
		"chrome",
		"firefox",
		"edge",
		"ie"
	],
	
	"capabilities": {
		"*": {
			"acceptSslCerts": true,
			"nativeEvents": true,
			"takesScreenshot": true,
			"loggingPrefs": {"browser": "ALL"}
		},
		"firefox": {
			"marionette": true
		},
		"ie": {
			"requireWindowFocus": true
		},
		"edge": {
		},
		"chrome": {
		}
	},
	
	/**
	 * Время ожидания выполнения тестов.
	 * Может быть установлено параметром запуска --timeout
	 */
	"timeout": 100000,
	
	/**
	 * Время ожидания загрузки страницы.
	 * По умолчанию = 1000.
	 */
	"explicit": 120000,
	
	/**
	 *
	 */
	"screenshots": "./test/screenshots",
	
	/**
	 *
	 */
	"logs": "./test/logs",
	
	/**
	 *
	 */
	//"baseurl": "http://pallada.lc/",
	"baseurl": "http://examples.sencha.com/extjs/6.2.0/examples/kitchensink/",
	
	/**
	 *
	 * Должно быть установлено параметром запуска --delay
	 */
	"delay": true
	
    /*
     *
     *
     */
}
